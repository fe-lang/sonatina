target = "evm-ethereum-london"

func public %entry() {
    block0:
        v0.i256 = evm_call_data_load 0.i32;
        v1.i32 = shr 224.i32 v0;
        v2.i32 = shl 32.i32 v0;
        v3.i32 = shr 224.i32 v2;
        v4.i32 = call %add v1 v3;

        mstore 0.i32 v4 i32;
        evm_return 0.i8 4.i8;
}

func public %add(v0.i32, v1.i32) -> i32 {
    block0:
        v2.i32 = add v0 v1;
        return v2;
}
