target = "evm-ethereum-london"

func public %entry() {
    block0:
        v0.i32 = call %prefer_backedge_layout 11.i32 22.i32;

        mstore 0.i32 v0 i32;
        evm_return 0.i8 32.i8;
}

func private %prefer_backedge_layout(v0.i32, v1.i32) -> i32 {
    block0:
        jump block1;

    block1:
        v2.i32 = phi (0.i32 block0) (v5 block3);
        v3.i1 = lt v2 3.i32;
        br v3 block2 block4;

    block2:
        v4.i32 = add v0 v1;
        v5.i32 = add v2 1.i32;
        jump block3;

    block3:
        jump block1;

    block4:
        return v2;
}
