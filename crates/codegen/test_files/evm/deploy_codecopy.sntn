#! evm.case: ok
#!   calldata: u32be(11) u32be(22)
#!   expect: return u256(33)

target = "evm-ethereum-osaka"

func public %entry() {
    block0:
        v0.i256 = evm_calldata_load 0.i32;
        v1.i32 = shr 224.i32 v0;
        v2.i32 = shl 32.i32 v0;
        v3.i32 = shr 224.i32 v2;
        v4.i32 = add v1 v3;
        mstore 0.i32 v4 i32;
        evm_return 0.i8 32.i8;
}

func public %init() {
    block0:
        v0.i256 = sym_addr &runtime;
        v1.i256 = sym_size &runtime;
        evm_code_copy 0.i256 v0 v1;
        evm_return 0.i256 v1;
}

object @Contract {
  section init {
    entry %init;
    embed .runtime as &runtime;
  }
  section runtime {
    entry %entry;
  }
}
