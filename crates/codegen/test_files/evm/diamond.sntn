target = "evm-ethereum-london"

func public %entry() {
    block0:
        v0.i256 = evm_calldata_load 0.i32;
        v1.i32 = shr 224.i32 v0;
        v2.i1 = gt v1 0.i32;
        br v2 block1 block2;

    block1:
        v3.i32 = add v1 1.i32;
        jump block3;

    block2:
        v4.i32 = add v1 2.i32;
        jump block3;

    block3:
        v5.i32 = phi (v3 block1) (v4 block2);

        mstore 0.i32 v5 i32;
        evm_return 0.i8 32.i8;
}
