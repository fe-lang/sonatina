#! evm.case: ok
#!   calldata: u32be(11) u32be(22)
#!   expect: return u256(24)

target = "evm-ethereum-osaka"

func public %entry() {
    block0:
        v0.i256 = evm_calldata_load 0.i32;
        v1.i32 = shr 224.i32 v0;
        v2.i1 = gt v1 0.i32;
        br v2 block1 block2;

    block1:
        v3.i32 = add v1 1.i32;
        jump block3;

    block2:
        v4.i32 = add v1 2.i32;
        jump block3;

    block3:
        v5.i32 = phi (v3 block1) (v4 block2);
        v6.i32 = phi (v3 block1) (v4 block2);
        v7.i32 = add v5 v6;

        mstore 0.i32 v7 i32;
        evm_return 0.i8 32.i8;
}

object @Contract {
  section runtime {
    entry %entry;
  }
}
