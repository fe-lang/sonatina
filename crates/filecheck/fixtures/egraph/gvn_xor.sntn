# Test: GVN via commutativity for xor combined with xor_self rule
# (x ^ 0) and (0 ^ x) both simplify to x, then x ^ x = 0
# sameln: func public %gvn_xor(v0.i32) -> i32 {
# check:         return 0.i32;
func public %gvn_xor(v0.i32) -> i32 {
    block0:
        v1.i32 = xor 0.i32 v0;
        v2.i32 = xor v0 0.i32;
        v3.i32 = xor v1 v2;
        return v3;
}