target = "evm-ethereum-london"

# Test: phi-of-same collapses after simplifying incoming values.
# sameln: func public %phi_of_same(v0.i32, v1.i1) -> i32 {
# check:         return v0;
func public %phi_of_same(v0.i32, v1.i1) -> i32 {
    block0:
        br v1 block1 block2;

    block1:
        v2.i32 = add v0 0.i32;
        jump block3;

    block2:
        jump block3;

    block3:
        v3.i32 = phi (v2 block1) (v0 block2);
        return v3;
}
