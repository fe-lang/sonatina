#! Simple loop with const condition.

target = "evm-ethereum-london"

# sameln: func public %const_loop() -> i8 {
# nextln:     block0:
# nextln:         return 11.i8;
func public %const_loop() -> i8 {
    block0:
        jump block1;

    block1:
        v1.i8 = phi (1.i8 block0) (v2 block1);
        v2.i8 = add v1 10.i8;
        v3.i1 = gt v2 v1;
        br v3 block2 block1;

    block2:
        return v2;
}
