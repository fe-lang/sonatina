#! Assert no constant is never folded.

target = "evm-ethereum-london"

# sameln: func public %non_folding(v0.*i64) {
# nextln:    block1:
# nextln:        return;
func public %non_folding(v0.*i64) -> unit {
    block1:
        v1.i64 = evm_sload v0;
        v2.i64 = add 1.i64 v0;
        v3.i64 = add -1.i64 v1;
        v4.i64 = mload v0 i64;
        v5.*i64 = add v4 8.i256;
        return;
}
