#! SCCP companion simplifier: self-identity must not fire on `Bot` values.

target = "evm-ethereum-london"

# sameln: func public %self_identity_bot(v0.i1) -> i1 {
# check: block3:
# nextln:         v1.i8 = phi (undef.i8 block0) (undef.i8 block2);
# nextln:         v2.i1 = eq v1 v1;
# nextln:         return v2;
func public %self_identity_bot(v0.i1) -> i1 {
    block0:
        br v0 block1 block2;

    block1:
        jump block3;

    block2:
        jump block3;

    block3:
        v1.i8 = phi (undef.i8 block1) (undef.i8 block2);
        v2.i1 = eq v1 v1;
        return v2;
}
